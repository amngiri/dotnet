@using TFLStore.Models;

@{
    ViewBag.Title = "Index";
}

<h2>My Shopping Cart</h2>
@{ 
    foreach(Item item in ViewBag.cart)
    {
            <p> Item Id= @item.ProductId      Quantity=@item.Quantity <a href="/shoppingcart/edit/@item.ProductId">Edit</a> |<a href="/shoppingcart/remove/@item.ProductId">remove</a> </p>
    }
}

